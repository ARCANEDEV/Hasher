{
    "name":        "arcanedev/hasher",
    "description": "Provides hash tools for Laravel.",
    "keywords": [
        "arcanedev", "laravel", "hashids", "hashid", "hash", "ids", "youtube", "bitly", "encrypt", "decrypt", "obfuscate"
    ],
    "homepage": "https://github.com/ARCANEDEV/Hasher",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/ARCANEDEV",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                 ">=5.5.9",
        "arcanedev/support":   "~3.0",
        "hashids/hashids":     "~1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpcov":      "~2.0",
        "phpunit/phpunit":     "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Hasher\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Hasher\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
